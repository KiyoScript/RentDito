<div class="d-flex justify-content-start align-items-center mb-6" >
  <div class="avatar me-3" >
    <img src="<%= avatar(caretaker.user) %>" alt="Avatar" class="rounded-circle">
  </div>
  <div class="d-flex flex-column" >
    <a href="app-user-view-account.html" class="text-body text-nowrap">
      <h6 class="mb-0"><%= caretaker.user.fullname %></h6>
    </a>
    <span>Gender: <%= gender_badge(caretaker.user.gender) %></span>
  </div>
</div>
<div class="d-flex justify-content-between" >
  <h6 class="mb-1 text-primary">Contact info</h6>
</div>
  <p class="mb-1">Email: <%= caretaker.user.email %></p>
  <p class="mb-0">Phone Number: <%= caretaker.user.phone_number %> </p>
<div class="divider">
  <div class="divider-text"></div>
</div>
